<h1>Item Catalog</h1>

<%= link_to "Add Item", new_item_path %>

<div>
<% @items.each do |item| %>
  <p>
  <%= item.name %>
  <%= link_to "Show", item_path(item) %>
  <%= link_to "Edit", edit_item_path(item) %>
  <%= link_to 'Destroy', item_path(item),
                      method: :delete, data: { confirm: 'Delete?' } %>
  </p>
<% end %>
</div>

